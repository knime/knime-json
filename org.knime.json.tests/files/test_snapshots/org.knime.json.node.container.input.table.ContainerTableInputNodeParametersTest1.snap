            "type" : "string",
            "default" : "{\"table-spec\":[{\"column-string\":\"string\"},{\"column-int\":\"int\"},{\"column-double\":\"double\"},{\"column-long\":\"long\"},{\"column-boolean\":\"boolean\"},{\"column-localdate\":\"localdate\"},{\"column-localdatetime\":\"localdatetime\"},{\"column-zoneddatetime\":\"zoneddatetime\"}],\"table-data\":[[\"value1\",1,1.5,1000,true,\"2018-03-27\",\"2018-03-27T08:30:45.111\",\"2018-03-27T08:30:45.111+01:00[Europe/Paris]\"],[\"value2\",2,2.5,2000,false,\"2018-03-28\",\"2018-03-28T08:30:45.111\",\"2018-03-28T08:30:45.111+01:00[Europe/Paris]\"]]}"
